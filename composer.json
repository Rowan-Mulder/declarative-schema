{
    "name": "rowanmulder/declarative-schema",
    "version": "1.5.0",
    "description": "A fork from micheljonkman/declarative-schema",
    "type": "library",
    "keywords": [
        "RowanMulder",
        "dbal-schema"
    ],
    "homepage": "https://github.com/rowan-mulder/declarative-schema",
    "license": "MIT",
    "authors": [
        {
            "name": "Rowan Mulder",
            "email": "rowanmulder94@hotmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "doctrine/dbal": "^4.0.1",
        "php-di/php-di": "^7.0.6",
        "symfony/console": "^6.0|^7.0",
        "jawira/case-converter": "^3.5.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "RowanMulder\\DeclarativeSchema\\": "src"
        }
    },
    "bin": [
        "bin/schema"
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {}
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
